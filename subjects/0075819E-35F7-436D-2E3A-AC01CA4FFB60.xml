    <arch:observation obsNumber="1">
      <arch:properties>
        <arch:property>
          <arch:variableID>166800ED-B19B-4057-C8DC-3DB395E33157</arch:variableID>
          <arch:valueID>3C5CE883-B4B5-40ED-6B22-B05E525ECDCD</arch:valueID>
          <oc:propid href="http://opencontext.org/properties/080A0B4A-EF5E-4BA9-EB41-26520E4A71E6">080A0B4A-EF5E-4BA9-EB41-26520E4A71E6</oc:propid>
          <oc:var_label type="alphanumeric">Location</oc:var_label>
          <oc:show_val>In the well to the southwest in SP 84, near the bottom of the floor in a very hard, tightly-packed lens that runs for the extent of the wells length and width.</oc:show_val>
        </arch:property>
        <arch:property>
          <arch:variableID>803FEE18-2CA3-4F6A-143E-62CBFC44B951</arch:variableID>
          <arch:valueID>F522713B-F791-4037-A04F-A1CD4B39A839</arch:valueID>
