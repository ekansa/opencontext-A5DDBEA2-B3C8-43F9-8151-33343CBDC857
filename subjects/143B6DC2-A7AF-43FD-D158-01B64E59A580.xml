          <oc:var_label type="nominal">Stage Description</oc:var_label>
          <oc:show_val>Major collapse denoted by thick layers of fill full of architectural fragments.</oc:show_val>
        </arch:property>
        <arch:property>
          <arch:variableID>C78CE4B9-7ECD-4AB4-72E9-07C348450681</arch:variableID>
