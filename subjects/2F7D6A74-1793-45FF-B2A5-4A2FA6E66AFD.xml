          <oc:var_label type="alphanumeric">Abuts</oc:var_label>
          <oc:show_val>Locus 97 threshold between Room 4 and 5, Locus 96 Room 4 north wall/ Room 5 south wall.</oc:show_val>
        </arch:property>
        <arch:property>
          <arch:variableID>0C351ACF-AD2B-4D9E-CCE4-AAA50DA8A986</arch:variableID>
